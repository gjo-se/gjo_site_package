                                        <numIndex index="0">LLL:EXT:gjo_site_package/Resources/Private/Language/ContentElements.xlf:flexform.carousel.option.loop</numIndex>
                                        <numIndex index="1">loop</numIndex>
                                    </numIndex>
                                </items>
                                <size>6</size>
                                <minitems>0</minitems>
                                <maxitems>8</maxitems>
                            </config>
                        </TCEforms>
